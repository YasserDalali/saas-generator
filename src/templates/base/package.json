{
  "name": "base",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon --exec \"ts-node --transpile-only\" src/app.ts",
    "build": "tsc",
    "lint": "eslint src/app.ts",
    "lint:fix": "eslint src/app.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@arcjet/inspect": "^1.0.0-beta.9",
    "@arcjet/node": "^1.0.0-beta.9",
    "chalk": "^5.4.1",
    "cookie-parser": "~1.4.7",
    "debug": "~4.4.1",
    "express": "~5.1.0",
    "morgan": "~1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  }
}
